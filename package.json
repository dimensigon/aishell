  "description": "AI-powered database management shell with MCP integration",
  "main": "dist/cli/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "ai-shell": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
