{
  "environment": "development",
  "logging": {
    "level": "debug",
    "pretty": true,
    "file": "logs/development.log"
  },
  "database": {
    "default": "postgresql",
    "enableQueryLogging": true,
    "slowQueryThreshold": 1000
  },
  "ai": {
    "provider": "ollama",
    "model": "llama3.2",
    "temperature": 0.7,
    "maxTokens": 4096
  },
  "security": {
    "requireApproval": true,
    "dangerousOperations": ["DROP", "DELETE", "TRUNCATE", "ALTER"],
    "allowedHosts": ["localhost", "127.0.0.1"]
  },
  "features": {
    "cognitive": true,
    "vectorSearch": true,
    "multiDatabase": true,
    "mcp": true
  }
}
