{
  "name": "@your-username/ai-shell",
  "version": "1.0.0",
  "description": "AI-powered database management shell with MCP integration",
  "main": "dist/cli/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "ai-shell": "./dist/cli/index.js",
    "aishell-grafana": "./dist/cli/grafana-integration.js",
    "ai-shell-mcp-server": "./dist/mcp/server.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "grafana": "ts-node src/cli/grafana-integration.ts",
    "grafana:setup": "ts-node src/cli/grafana-integration.ts setup",
    "grafana:deploy": "ts-node src/cli/grafana-integration.ts deploy-dashboards",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "npm run test:db:setup && npm test tests/integration/ && npm run test:db:teardown",
    "test:db:setup": "bash scripts/setup-test-dbs.sh",
    "test:db:teardown": "bash scripts/teardown-test-dbs.sh",
    "test:db:clean": "bash scripts/teardown-test-dbs.sh --volumes",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "prepack": "bash scripts/prepublish.sh"
  },
  "keywords": [
    "ai",
    "shell",
    "mcp",
    "database",
    "llm",
    "cli",
    "repl",
    "postgresql",
    "mysql",
    "mongodb",
    "redis",
    "grafana",
    "prometheus",
    "query-optimizer",
    "database-management",
    "natural-language",
    "sql",
    "nosql"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-shell.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/ai-shell/issues"
  },
  "homepage": "https://github.com/your-username/ai-shell#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@slack/web-api": "^7.12.0",
    "@types/blessed": "^0.1.25",
    "axios": "^1.6.0",
    "blessed": "^0.1.81",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "eventemitter3": "^5.0.1",
    "fs-extra": "^11.3.2",
    "inquirer": "^12.10.0",
    "ioredis": "^5.8.2",
    "mongodb": "^6.20.0",
    "mysql2": "^3.15.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-saml": "^4.0.4",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.19.23",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-saml": "^1.1.10",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "archiver": "^7.0.1",
    "csv-parse": "^6.1.0",
    "eslint": "^8.56.0",
    "fast-csv": "^5.0.5",
    "oracledb": "^6.10.0",
    "rollup": "^4.52.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.4"
  }
}
